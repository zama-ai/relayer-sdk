{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug CLI public-decrypt",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "npx",
      "runtimeArgs": [
        ".",
        "test",
        "public-decrypt",
        "--types",
        "euint32",
        "--fetch-retries",
        "10",
        "--fetch-retry-delay",
        "2000",
        "--debug"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}
